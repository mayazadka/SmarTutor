<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TutorSignUp">

    <TextView
        android:id="@+id/signUpTutor_birthday_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/quicksand_bold"
        android:text="Birthday Date"
        android:textSize="@dimen/text_size"
        app:layout_constraintStart_toStartOf="@+id/signUpTutor_gender_tv"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_gender_tv" />

    <EditText
        android:id="@+id/signUpTutor_firstname_et3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="@font/quicksand_bold"
        android:hint="Your Professions"
        android:inputType="textPersonName"
        android:textSize="@dimen/text_size"
        app:layout_constraintStart_toStartOf="@+id/editTextTextMultiLine"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextMultiLine" />

    <TextView
        android:id="@+id/signUpTutor_firstname_tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/quicksand_bold"
        android:text="About Me"
        android:textSize="@dimen/text_size"
        app:layout_constraintStart_toStartOf="@+id/signUpTutor_professions_et"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_professions_et" />

    <TextView
        android:id="@+id/signUpTutor_professions_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/quicksand_bold"
        android:text="Professions"
        android:textSize="@dimen/text_size"
        app:layout_constraintStart_toStartOf="@+id/signUpTutor_professions_et"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_birthday_tv" />

    <TextView
        android:id="@+id/signUpTutor_gender_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/quicksand_bold"
        android:text="Gender"
        android:textSize="@dimen/text_size"
        app:layout_constraintStart_toStartOf="@+id/signUpTutor_firstname_et"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_firstname_et" />

    <EditText
        android:id="@+id/signUpTutor_professions_et"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:fontFamily="@font/quicksand_bold"
        android:hint="Your Professions"
        android:inputType="textPersonName"
        android:textSize="@dimen/text_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_professions_tv" />

    <EditText
        android:id="@+id/signUpTutor_firstname_et"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:fontFamily="@font/quicksand_bold"
        android:hint="Your first name"
        android:inputType="textPersonName"
        android:textSize="@dimen/text_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_firstname_tv" />

    <EditText
        android:id="@+id/signUpTutor_lastname_et"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:fontFamily="@font/quicksand_bold"
        android:hint="Your last name"
        android:inputType="textPersonName"
        android:textSize="@dimen/text_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_lastname_tv" />

    <TextView
        android:id="@+id/signUpTutor_firstname_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/quicksand_bold"
        android:text="First Name"
        android:textSize="@dimen/text_size"
        app:layout_constraintStart_toStartOf="@+id/signUpTutor_lastname_et"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_lastname_et" />

    <TextView
        android:id="@+id/signUpTutor_lastname_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/quicksand_bold"
        android:text="Last Name"
        android:textSize="@dimen/text_size"
        app:layout_constraintStart_toStartOf="@+id/signUpTutor_email_et"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_email_et" />

    <TextView
        android:id="@+id/signUpTutor_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/quicksand_bold"
        android:text="Sign Up - Tutor"
        android:textSize="@dimen/title_size"
        app:layout_constraintStart_toStartOf="@+id/signUpTutor_email_tv"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/signUpTutor_email_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/quicksand_bold"
        android:text="Email"
        android:textSize="@dimen/text_size"
        app:layout_constraintStart_toStartOf="@+id/signUpTutor_email_et"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_title_tv" />

    <EditText
        android:id="@+id/signUpTutor_email_et"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:fontFamily="@font/quicksand_bold"
        android:hint="Your email address"
        android:inputType="textPersonName"
        android:textSize="@dimen/text_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_email_tv" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="0dp"
        android:layout_height="58dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Tell about yourself"
        android:inputType="textMultiLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signUpTutor_firstname_tv4" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="-30dp" />

</androidx.constraintlayout.widget.ConstraintLayout>